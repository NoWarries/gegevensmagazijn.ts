    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
